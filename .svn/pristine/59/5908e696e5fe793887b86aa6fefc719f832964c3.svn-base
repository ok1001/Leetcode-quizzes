vector<string> summaryRanges(vector<int> &nums)
{
    vector<string> res;
    int left = 0, right = 0;

    for (int i = 0; i < nums.size(); ++i)
    {
        if (i < nums.size() - 1 && nums[i] + 1 == nums[i + 1])
        {
            right = i + 1;
        }
        else
        {
            string s;
            if (left == right)
            {
                s = to_string(nums[left]);
            }
            else
            {
                s = to_string(nums[left]) + "->" + to_string(nums[right]);
            }
            res.emplace_back(s);
            left = right = i + 1;
        }
    }

    return res;
}
